------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Content-Type: text/ecl
Entry-Name: .content

start-cooperate-repository
try {  
    create-cooperate-project testCooperateProject
    
    get-menu "File/New/Usecase Diagram" | click
    
    with [get-window -class WizardDialog] {
        get-tree | select testCooperateProject
        get-editbox -after [get-label "Project:"] | set-text testCooperateProject
        get-editbox -after [get-label "Model Name:"] | set-text myModel
        get-editbox -after [get-label "Diagram Name:"] | set-text myDiagram
        get-button Finish | click
    }
    
    with [get-view "Project Explorer" | get-tree] {
    	wait -ms 500
    	select "testCooperateProject" | get-menu -path Refresh | click
        get-item "testCooperateProject/model-gen/myModel" | get-property caption | equals myModel | verify-true
        get-item "testCooperateProject/model-gen/myModel - myDiagram.uc" | get-property caption | equals "myModel - myDiagram.uc" | verify-true
        get-item "testCooperateProject/models" | get-property childCount | equals 1 | verify-true
    }
} -error [val errorObj] -catch{
	log-error $errorObj
} -finally {
    stop-cooperate-repository
}
