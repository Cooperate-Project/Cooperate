Entry-Name: .content

start-cooperate-repository
try {
    create-cooperate-project testCooperateProject
    create-class-diagram testCooperateProject myDiagram
    
    get-view "Project Explorer" | get-tree | select testCooperateProject | get-menu "Delete from Repository" | click
    get-window "Delete Resources" | get-button OK | click
    get-menu "File/Import..." | click
    get-window Import | get-tree | select "Cooperate/Import Cooperate Project" | double-click
    with [get-window "Import Cooperate Project"] {
        get-group CDO | get-editbox -after [get-label "Repository:"] | set-text "ui-tests-repo"
        get-button "Next >" | click
}
    get-window "Import Cooperate Project" | get-table | get-property itemCount | equals 0 | verify-true
} -finally {
    stop-cooperate-repository
}
