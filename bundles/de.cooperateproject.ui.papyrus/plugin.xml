<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="de.cooperateproject.ui.editors.launcher.extensions.editorlauncher">
      <provider
            class="de.cooperateproject.ui.papyrus.editorlauncher.PapyrusCDOLauncherFactory">
      </provider>
   </extension>
   <extension
         point="org.eclipse.papyrus.infra.core.sashModelProvider">
      <sashModelProvider
            class="de.cooperateproject.ui.papyrus.editorlauncher.DummySashModelProvider"
            scheme="cdo.checkout">
      </sashModelProvider>
   </extension>
   <extension
         point="de.cooperateproject.ui.focus.manager.factory">
      <factory
            factory="de.cooperateproject.ui.papyrus.editorlauncher.FocusManagerGraphicalFactory">
      </factory>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="de.cooperateproject.ui.focus.command.sendFocus"
            contextId="org.eclipse.gmf.runtime.diagram.ui.diagramContext"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="CTRL+ALT+F">
      </key>
      <key
            commandId="de.cooperateproject.ui.focus.command.mute"
            contextId="org.eclipse.gmf.runtime.diagram.ui.diagramContext"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="CTRL+ALT+M">
      </key>
   </extension>
</plugin>
