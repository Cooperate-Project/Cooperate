<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension-point id="de.cooperateproject.modeling.textual.cls.formatter" name="ClsFormatter" schema="schema/de.cooperateproject.modeling.textual.cls.formatter.exsd"/>
   <extension
         point="de.cooperateproject.modeling.textual.cls.formatter">
      <client
            class="de.cooperateproject.modeling.textual.cls.formatting2.ClsFormatter"
            priority="1">
      </client>
   </extension>
   <extension
         point="de.cooperateproject.modeling.textual.xtext.runtime.automatedissueresolutionfactories">
      <factory
            class="de.cooperateproject.modeling.textual.cls.issues.ClsUMLReferencingElementMissingElementFactory">
      </factory>
      <factory
            class="de.cooperateproject.modeling.textual.cls.issues.ClsAssociationMemberEndRoleNameFactory">
      </factory>
      <factory
            class="de.cooperateproject.modeling.textual.cls.issues.ClsCardinalityCheckFactory">
      </factory>
      <factory
            class="de.cooperateproject.modeling.textual.cls.issues.ClsPropertyAbstractQualifierFactory">
      </factory>
      <factory
            class="de.cooperateproject.modeling.textual.cls.issues.ClsPropertyStaticQualifierFactory">
      </factory>
      <factory
            class="de.cooperateproject.modeling.textual.cls.issues.ClsRootPackageMissingFactory">
      </factory>
   </extension>
   <extension
         point="de.cooperateproject.modeling.textual.xtext.runtime.derivedstate.calculator">
      <calculator
            class="de.cooperateproject.modeling.textual.cls.derivedstate.calculator.AssociationMemberEndCalculator">
      </calculator>
      <calculator
            class="de.cooperateproject.modeling.textual.cls.derivedstate.calculator.GeneralizationCalculator">
      </calculator>
      <calculator
            class="de.cooperateproject.modeling.textual.cls.derivedstate.calculator.ImplementationCalculator">
      </calculator>
      <calculator
            class="de.cooperateproject.modeling.textual.cls.derivedstate.calculator.XtextAssociationCalculator">
      </calculator>
   </extension>
   <extension
         point="de.cooperateproject.modeling.textual.xtext.runtime.derivedstate.remover">
      <remover
            class="de.cooperateproject.modeling.textual.cls.derivedstate.remover.XtextAssociationRemover">
      </remover>
   </extension>

</plugin>
