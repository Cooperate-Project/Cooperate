<?xml version="1.0" encoding="UTF-8"?>
<cls:ClassDiagram
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:cls="http://www.cooperateproject.de/modeling/textual/cls/Cls"
    xsi:schemaLocation="http://www.cooperateproject.de/modeling/textual/cls/Cls cls.ecore"
    title="Class Diagram">
  <rootPackage
      name="rootElement">
    <packages
        xsi:type="cls:Package"
        name="Car">
      <packages
          xsi:type="cls:Package"
          name="Accessory">
        <classifiers
            xsi:type="cls:Class"
            name="Stereo"/>
        <classifiers
            xsi:type="cls:Interface"
            name="OptionalPart"/>
      </packages>
      <packageImports
          importedPackage=""/>
      <classifiers
          xsi:type="cls:Class"
          name="Car"/>
      <classifiers
          xsi:type="cls:Class"
          name="Chassis"/>
    </packages>
    <packages
        xsi:type="cls:Package"
        name="People">
      <classifiers
          xsi:type="cls:Class"
          name="Person"/>
      <connectors
          xsi:type="cls:XtextAssociation"
          name="owns">
        <memberEnds
            name="person"
            type="//@rootPackage/@packages.1/@classifiers.0">
          <cardinality
              lowerBound="1"
              upperBound="1"/>
        </memberEnds>
        <memberEnds
            name="car"
            type="//@rootPackage/@packages.0/@classifiers.0"
            navigable="true">
          <cardinality
              upperBound="-1"/>
        </memberEnds>
      </connectors>
      <connectors
          xsi:type="cls:XtextAssociation"
          name="wantsToUse">
        <memberEnds
            name="person"
            type="//@rootPackage/@packages.1/@classifiers.0"/>
        <memberEnds
            name="optionalpart"
            type="//@rootPackage/@packages.0/@packages.0/@classifiers.1">
          <cardinality
              upperBound="-1"/>
        </memberEnds>
      </connectors>
    </packages>
  </rootPackage>
</cls:ClassDiagram>
