<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="de.cooperateproject.modeling.common.types">
      <type
            name="Activity"
            papyrusDiagramKind="org.eclipse.papyrus.uml.diagram.activity"
            papyrusDiagramType="PapyrusUMLActivityDiagram"
            textualFileExtension="act">
      </type>
   </extension>
   <extension
         point="de.cooperateproject.modeling.transformation.registry.transformationfactories">
      <transformation>
         <transformationfolder
               foldername="transforms">
         </transformationfolder>
         <defaulttransformation
               diagramName="Activity"
               graphicalFileExtension="notation"
               textualFileExtension="act">
         </defaulttransformation>
      </transformation>
   </extension>

</plugin>
